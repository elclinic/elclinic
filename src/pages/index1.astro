---
import BaseLayout from '../layouts/BaseLayout.astro';
import About from '../components/About.astro';
import Services from '../components/Services.astro';
import Doctors from '../components/Doctors.astro';
import Reviews from '../components/Reviews.astro';
import Hero from '../components/Hero.astro';
import Price from '../components/Price.astro';
import Map from '../components/Map.astro';
import ServicesList from '../components/ServicesList.astro';
import ConsultForm from '../components/ConsultForm.astro';
---

<BaseLayout title="El Clinic Dental - Стоматологія Запоріжжя">
    <Hero />
    <ConsultForm />
    <About />
    <Services />
    <Doctors />
    <ServicesList />
    <Reviews />
    <Price />
    <Map
            query="Стоматологія El Clinic Dental"
            title="Стоматологічна клініка El Clinic Dental"
            zoom={15}
            type="m"
    />
</BaseLayout>